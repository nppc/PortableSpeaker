                                 ;*
                                 ;* "div16u" - 16/16 Bit Unsigned Division
                                 ;*
                                 ;* This subroutine divides the two 16-bit numbers 
                                 ;* "tmpH1:tmpL1" (dividend) and "tmpH2:tmpL2" (divisor). 
                                 ;* The result is placed in "tmpH1:tmpL1" and the remainder in
                                 ;* "tmp3:tmp2".
                                 ;*  
                                 ;* Number of words	:19+reminder routine (7)
                                 ;* Number of cycles	:235/251 (Min/Max)
                                 ;* Low registers used	:6 (tmpL1,tmpH1,tmpL2,tmpH2,tmp2,tmp3)
                                 ;* High registers used  :1 (tmp)
                                 ;*
                                 ;***************************************************************************
                                 ; about 25us at 9.6mhz
000051 2433                      div16u:	clr	tmp2	;clear remainder Low byte
000052 1844                      	sub	tmp3,tmp3;clear remainder High byte and carry
000053 e101                      	ldi	tmp,17	;init loop counter
000054 1c88                      d16u_1:	rol	tmpL1		;shift left dividend
000055 1c99                      	rol	tmpH1
000056 950a                      	dec	tmp		;decrement counter
000057 f441                      	brne	d16u_2		;if done
                                 	; take care of reminder (we want to have coeff as precise as possible)
                                 	;divide divisor by 2
000058 94b6                      	lsr tmpH2
000059 94a7                      	ror tmpL2
                                 	; compare reminder to half of divisor
00005a 143a                      	cp  tmp2, tmpL2 
00005b 044b                      	cpc	tmp3, tmpH2
00005c f010                      	brlo d16u_exit
                                 	; increment result
00005d 0c81                      	add tmpL1, z1
00005e 1c90                      	adc tmpH1, z0
                                 d16u_exit:
00005f 9508                      	ret			;    return
000060 1c33                      d16u_2:	rol	tmp2	;shift dividend into remainder
000061 1c44                      	rol	tmp3
000062 183a                      	sub	tmp2,tmpL2	;remainder = remainder - divisor
000063 084b                      	sbc	tmp3,tmpH2	;
000064 f420                      	brcc	d16u_3		;if result negative
000065 0c3a                      	add	tmp2,tmpL2	;    restore remainder
000066 1c4b                      	adc	tmp3,tmpH2
000067 9488                      	clc			;    clear carry to be shifted into result
000068 cfeb                      	rjmp	d16u_1		;else
000069 9408                      d16u_3:	sec			;    set carry to be shifted into result
