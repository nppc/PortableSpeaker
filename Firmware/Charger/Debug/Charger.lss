000043 9936                      		sbic ADCSRA, ADSC
000044 cffe                      		rjmp waitforADC_Ref
000045 b034                      		in tmp2, ADCL
000046 b045                      		in tmp3, ADCH
000047 950a                      		dec tmp
000048 f7c9                      		brne waitforADC_Ref_L1
